{
  "expo": {
    "name": "oto-app",
    "slug": "oto",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "otoapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.otoapp",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app needs access to your microphone to record audio for voice analysis and transcription."
      },
      "icon": {
        "image": "./assets/images/icon.png",
        "prerendered": true
      },
      "splash": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "privyAppId": "cmcc0wydx0086js0l5e5gj35e",
      "privyClientId": "client-WY6N2jYhjSc3MBZXJinPQz9XWMUCQsJie5MPqjhmHi2mr",
      "eas": {
        "projectId": "1da5ac4c-aa75-4cbc-876c-21334330f304"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-web-browser",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  },
  "owner": "leq6c"
}